{
  "name": "ipalaus/geonames",
  "description": "A collection of models and commands to get all the power of GeoNames in Eloquent ORM.",
  "keywords": [
    "geonames",
    "location",
    "eloquent",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Isern Palaus",
      "email": "ipalaus@ipalaus.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "ext-zip": "*",
    "illuminate/config": ">=5.3.0",
    "illuminate/console": ">=5.3.0",
    "illuminate/database": ">=5.3.0",
    "illuminate/filesystem": ">=5.3.0",
    "illuminate/support": ">=5.3.0",
    "symfony/process": "3.1.*"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
  },
  "autoload": {
    "classmap": [
      "src/migrations"
    ],
    "psr-4": {
      "Ipalaus\\Geonames\\": "src/"
    }
  },
  "minimum-stability": "dev"
}
